      "id": "menu-item-Minha-conta",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "blockClass": ["menu-mobile", "menu-mobile-drawer"],
      "itemProps": {
        "type": "internal",
        "href": "",
        "noFollow": false,
        "tagTitle": "Minha-conta",
        "text": "Minha-conta"
      }
    },
    "blocks": [
      "vtex.menu@2.x:submenu.accordion#Minha-conta-mobile-accordion"
    ]
  },
  "vtex.menu@2.x:submenu.accordion#Minha-conta-mobile-accordion": {
    "children": [
      "vtex.menu@2.x:menu#Minha-conta-mobile"
    ]
  },
  "vtex.menu@2.x:menu#Minha-conta-mobile": {
