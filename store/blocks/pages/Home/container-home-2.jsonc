{
    "flex-layout.row#container-home-2":{
        "props":{
            "blockClass": "container-home-list-user",
            "colSizing": "auto"
        },
        "children":[
            "flex-layout.col#container-home-title",
            "flex-layout.row#container-home-body",
            "image#wallpaper"
        ]
    },
    "flex-layout.col#container-home-title":{
        "props":{
            "blockClass": "container-home-title"
        },
        "children":[
            "rich-text#title"
        ]
    },
    "rich-text#title": {
        "props": {
          "text": "Conheça a opinião de quem usa no dia-dia",
          "blockClass": "title-home-2"
        }
    },
    "flex-layout.row#container-home-body":{
        "props":{
            "blockClass": "container-home-slider"
        },
        "children":[
            "list-context.info-card-list#slider-user"
        ]
    },
    "list-context.info-card-list#slider-user": {
