        ]
    },
    "logo#plans-2": {
        "props": {
          "title": "Logo",
          "href": "/e-mail",
          "url": "assets/images/user.png",
          "blockClass": "logo-signature"
        }
      },
    "link.product#plans-2": {
        "props": {
          "href": "/e-mail",
